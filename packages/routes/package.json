{
  "name": "routes",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": null
    }
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external react,@remix-run/react",
    "dev": "tsup src/index.ts --format esm --watch --dts --external react,@remix-run/react",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "glob": "^10.3.10"
  },
  "peerDependencies": {
    "@remix-run/dev": "^2.0.1"
  }
}
